<script type="text/javascript">
$(document).ready(function(){
    if($('.take-fit-quiz').length > 0){
        var $data = $().add('<div id="fq-wrap">'); // Create empty object
        $($data).load('/pages/fq #fit-quiz-top', function(){
            $('.wrapper').prepend($data);
            $('.select-box').styleddropdown();
        // load fq script
        $.getScript('{{ 'fq.min.js' | asset_url }}');
        $.getScript('{{ 'quirks.js' | asset_url }}',function(data,textStatus,jqxhr){
            $('.take-fit-quiz').click(function(){
        	    $('#fit-quiz-top').slideDown(600);
			    return false;
		    });
        });
        });

         {% if template == 'product' %}
        $.getScript('{{ 'jq_select.js' | asset_url }}');
        {% endif %}        
    } // Load the fitquiz data when the id is found on the page.
    {% if template == 'collection.looks' %}
        $('#product-select option').each(function(){
            var fq_top = $.cookie('fq_top').substring(0,2);
            var fq_bust = $.cookie('fq_bust').substr(2,1)+'/'+$.cookie('fq_bust').substr(3,1);
            var fq_length = $.cookie('fq_length');
            var fq_waist = $.cookie('fq_waist');
    
            if($(this).text().indexOf(fq_length) > 0 && $(this).text().indexOf(fq_top) > 0 && $(this).text().indexOf(fq_bust) > 0 ){
        	    $(this).attr('selected',true);
	            }//else if($(this).text().indexOf(fq_waist) > 0 && $(this).text().indexOf(fq_length) > 0 ){
			    //$(this).attr('selected',true);
	       //}
            });
    {% endif %}
});
</script>
