  {% include 'olark' %}
  {% if product %}
  <script type="text/javascript">
  var selectCallback = function(variant, selector) {
    var buyButton = $("#add-to-cart");
    var prod_sel0 = $("#product-select-option-0");
    var prod_sel1 = $("#product-select-option-1");
      if (variant && variant.available) {
        // selected a valid variant
        buyButton.removeClass('disabled').removeAttr('disabled'); // remove unavailable class from add-to-cart button, and re-enable button
        if(variant.inventory_quantity <= 0){
          buyButton.val("Add to Bag").removeClass("sold-out default")
                                     {% unless product.type == "Pants" or product.type == "Skirts" %}
                                       .addClass("default");
                                    {% endunless %}
          $("div#purchase p").css({ display: "none" });
          $("div#purchase p:last-child").css({ display: "inline-block" });
        }
        else{
          buyButton.val("Add to Bag").removeClass("pre-order sold-out default");
          $("div#purchase p").css({ display: "none" });
          {% case handle %}
            {% when 'ansley-dress-berry' %}
              $("div#purchase p").css({ display: "none" });
            {% when 'chelsea-blazer-herringbone' %}
             $("div#purchase p").css({ display: "none" });
            {% when 'chloe-shirt'%}
              $("div#purchase p").css({ display: "none" });
            {% when 'fiona-blouse' %}
              $("div#purchase p").css({ display: "none" });
            {% when 'elliott-pencil-skirt' %}
              $("div#purchase p:last-child").css({ display: "inline-block" });
            {% when 'elliott-sheath-black' %}
              $("div#purchase p").css({ display: "none" });
            {% when 'elliott-blazer-black' %}
              $("div#purchase p").css({ display: "none" });
            {% when 'elliott-trouser-black' %}
              $("div#purchase p:last-child").css({ display: "inline-block" });
            {% when 'elliott-pant-black' %}
              $("div#purchase p:last-child").css({ display: "inline-block" });
            {% when 'elliott-sheath-skirt' %}
              $("div#purchase p:last-child").css({ display: "inline-block" });
            {% when 'riley-sheath-navy' %} 
              $("div#purchase p").css({ display: "none" });
            {% when 'riley-blazer-cobalt' %}
              $("div#purchase p").css({ display: "none" });
            {% when 'riley-pant-cobalt' %}
              $("div#purchase p:last-child").css({ display: "inline-block" });
            {% when 'riley-skirt-cobalt' %} 
              $("div#purchase p:last-child").css({ display: "inline-block" });
          {% endcase %}
        }
      }
      else {
        if(prod_sel0.val() == "Length" || prod_sel1.val() == "Bust" || prod_sel0.val() == "Waist" || prod_sel1.val() == "Inseam" || prod_sel1.val() == "Length" || prod_sel1.val() == "Waist" || prod_sel0.val() == "Inseam"){
          buyButton.val("Add to Bag").removeClass("pre-order sold-out").addClass("default");
          $("div#purchase p").css({ display: "none" });
        }
        else{
          buyButton.val("Out of Stock").addClass("sold-out").removeClass("pre-order default");
          $("div#purchase p").css({ display: "none" });
        }
        // variant doesn't exist
        buyButton.addClass('disabled').attr('disabled', 'disabled'); // set add-to-cart button to unavailable class and disable button
        if(!$.cookie('fq_top') && !$.cookie('fq_length') && !$.cookie('fq_waist')){}
      }
      {% unless product.type == "Pants" or product.type == "Skirts" %}
      prod_sel1.change(function() {
              $("div.fits-like p").hide();
              if ($(":contains(32)", this).is(":selected")) {
                  $("div.fits-like p").eq(0).show();
              }
              else if ($(":contains(34)", this).is(":selected")) {
                  $("div.fits-like p").eq(1).show();
              }
              else if ($(":contains(36)", this).is(":selected")) {
                  $("div.fits-like p").eq(2).show();
              }
              else if ($(":contains(38)", this).is(":selected")) {
                  $("div.fits-like p").eq(3).show();
              }
              else if ($(":contains(40)", this).is(":selected")) {
                  $("div.fits-like p").eq(4).show();
              }
              else {}
          });
        {% endunless %}
      };
    // initialize multi selector for product
    $(function() {
      new Shopify.OptionSelectors(
        "product-select", 
        { product: {{ product | json }}, onVariantSelected: selectCallback }
      );  
    });
  </script>
  {% endif %}
  <script type="text/javascript">
    function clickclear(thisfield, defaulttext) {
    	if (thisfield.value == defaulttext) {
    	thisfield.value = "";
    	}
    	}

    	function clickrecall(thisfield, defaulttext) {
    	if (thisfield.value == "") {
    	thisfield.value = defaulttext;
    	}
    	}
  </script>
