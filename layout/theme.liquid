  {% include 'olark' %}
  {% if product %}
  <script type="text/javascript">
  var selectCallback = function(variant, selector) {
    var buyButton = $("#add-to-cart");
    var prod_sel0 = $("#product-select-option-0");
    var prod_sel1 = $("#product-select-option-1");
    var prod_sel2 = $("#product-select-option-2");
    // p with order delay messages
    var $delay = $(".order-delays");
    // p with out of stock messages
    var $nostock = $(".out-of-stock");
    // p for pre-order messages
    var $preorder = $(".pre-order")
    // var for showing appropriate p
    var show = { display: "inline-block" }
    
    // add js hook class to buy from button click
    buyButton.addClass("js-buy-button");

      if (variant) {
        // update variant-sku modal with sku
        // of currently selected variant
        var sku = variant.sku
        $(".variant-sku").empty().append(sku)
      }    
    
      if (variant && variant.available) {
          
        // selected a valid variant
        buyButton.removeClass("disabled bg-blue js-notify-hook").removeAttr("disabled"); // remove unavailable class from add-to-cart button, and re-enable button
        // has no inventory
        if(variant.inventory_quantity <= 0){
          $("div#purchase p").css({ display: "none" });
          {% include 'pants-skirts-inventory' %}
          {% case handle %}
            {% when 'ansley-dress-berry' or 'chelsea-blazer-herringbone' %}
              $delay.css(show);
          {% endcase %}
        }
        // has inventory
        else{
          {% include 'pants-in-stock-check' %}
        }
      }
      // variant not available
      else {
        // if on default select options don't allow purchase
        if(prod_sel0.val() == "Length" || prod_sel0.val() == "Bust" || prod_sel1.val() == "Bust" || prod_sel0.val() == "Waist" || prod_sel1.val() == "Inseam" || prod_sel1.val() == "Length" || prod_sel1.val() == "Waist" || prod_sel0.val() == "Inseam" || prod_sel2.val() == "Color"){
          buyButton.val("Add to Bag")
                   .addClass("default disabled")
                   .removeClass("pre-order sold-out")
                   .attr('disabled', 'disabled');
          $("div#purchase p").css({ display: "none" });
        }
        // variant not available
        else {
          buyButton.val("Out of Stock")
                   .addClass("sold-out")
                   .removeClass("pre-order default disabled js-buy-button bg-blue js-notify-hook");
          $("div#purchase p").css({ display: "none" });
          
        }
        // variant doesn't exist
      }
      {% unless product.type == "Pants" or product.type == "Skirts" %}
      prod_sel1.change(function() {
              $("div.fits-like p").hide();
              if ($(":contains(32)", this).is(":selected")) {
                  $("div.fits-like p").eq(0).show();
              }
              else if ($(":contains(34)", this).is(":selected")) {
                  $("div.fits-like p").eq(1).show();
              }
              else if ($(":contains(36)", this).is(":selected")) {
                  $("div.fits-like p").eq(2).show();
              }
              else if ($(":contains(38)", this).is(":selected")) {
                  $("div.fits-like p").eq(3).show();
              }
              else if ($(":contains(40)", this).is(":selected")) {
                  $("div.fits-like p").eq(4).show();
              }
              else {}
          });
        {% endunless %}
      };
    // initialize multi selector for product
    $(function() {
      new Shopify.OptionSelectors(
        "product-select", 
        { product: {{ product | json }}, onVariantSelected: selectCallback }
      );
      {% if product.options.size == 1 and product.options.first != "Title" and template != "product.gift-card" %}
        $(".selector-wrapper:eq(0)").prepend("<label>{{ product.options.first }}</label>")
      {% endif %}  
    });
  </script>
  {% endif %}
  <script type="text/javascript">
    function clickclear(thisfield, defaulttext) {
        if (thisfield.value == defaulttext) {
        thisfield.value = "";
    	}
    	}

    	function clickrecall(thisfield, defaulttext) {
    	if (thisfield.value == "") {
    	thisfield.value = defaulttext;
    	}
    	}
  </script>
