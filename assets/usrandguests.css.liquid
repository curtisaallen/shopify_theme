#customer-accounts, #reset-passrd{
    height: 690px;
}
#customer-login, #recover-password{
    top: 20px;
}
#reset-hdr{
    top: 13px;
}

#reset-passrd p{
    margin-top: 15px !important;
}

#customer-login, #recover-password, #reset-hdr{
    font-size: 11px;
    margin: 0;
    position: relative;
}

#customer-login{
    margin-right: 40px;
    width: 220px;
    float: left;
}
#reset-passrd form{
    width: 250px;
}

#recover-password{
    float: left;
    font-size: 10px;
    margin-right: 40px;
    width: 219px;
}

#recover-password p{
    margin-bottom: 14px;
}

#reset-passrd p, #reset-passrd form{
    font-size: 10px;
}

#customer-login form, .page-header h1, #recover-password, #guest, #reset-passrd{
    font-family: "museo-sans-n5","museo-sans" !important;
}

form#customer_login input, #recover-password form #recover_email input, #reset-passrd form #password input, #reset-passrd form #password_confirm input{
    border: solid 1px #e8e8e8;
}

#login_email, #reset-passrd form #password{
    margin-bottom: 8px;
}

#login_email input, #login_password input, #recover-password form #recover_email input, #reset-passrd form #password input, #reset-passrd form #password_confirm input{
    padding: 3px;
}

#login_email input{
    width: 169px;
}

#recover-password form #recover_email input{
    width: 172px;
}

#login_password input{
    width: 145px;
}

#login_email label, #login_password label, form #recover_email label{
    padding-right: 5px;
}

#reset-passrd form #password label{
    padding-right: 47px;
}

#reset-passrd form #password input, #reset-passrd form #password_confirm input{
    width: 137px;
}

#login_password, #recover-password form #recover_email, #reset-passrd form #password_confirm{
    margin-bottom: 24px;
}

.action-bottom input, .action-bottom div, #customer_login_guest input, #reset-passrd form .action_bottom input{
    clear: both;
    float: right;
}

.action-bottom input, #customer_login_guest input, #reset-passrd form .action_bottom input{
    background-color: #031d48;
}

.action-bottom input:hover, #customer_login_guest input:hover, #reset-passrd form .action_bottom input:hover{
    background-color: #e939af;
}
.action-bottom input:active, #customer_login_guest input:active, #reset-passrd form .action_bottom input:active{
    background-color: #cc0088;
}
#forgot_password a{
    color: #252525;
    font-style: normal;
    font-size: 10px;
}
#forgot_password a:hover {
    color: #e939af;
}
#forgot_password a:active {
    color: #cc0088;
}
.action-bottom span.note, .action-bottom span.note a{
    font-size: 10px;
}

.page-header h1{
    border: none;
    color: #031d48;
    font-size: 12px;
    font-style: normal !important;
}

.errors{
    color: #D3018D;
    margin-bottom: 16px;
}

/* #### Checkout guest #####*/

#guest{
    background: url(or.png) 0 0 no-repeat;
    display: block !important;
    float: right;
    margin-top: 0;
    padding-left: 56px;
    position: relative;
    top: 20px;
    margin-right: 117px;
    width: 276px;
}

#guest p{
    font-size: 11px;
    margin-bottom: 24px;
}

/**** USER ACCOUNTS****/

h2#saved-size{
