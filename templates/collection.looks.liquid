<!-- MAKE EMPTY COLLECTION CALLED look THAT USES THIS TEMPLATE -->
<section class="collection_looks">
  <div id="1" class="js-looks-div pointer">
    <img class="hero" src="{{ 'look1.jpg' | asset_url }}" />
    <div class="description top right white">
      <h4 class="serif">&ldquo;Stand out in a sea of gray and black&rdquo;</h4>
      <p>Riley blazer <span>$228</span>, Riley pencil skirt <span>$128</span></p>
      <input class="js-looks-button" type="button" name="Buy Outfit" value="Buy This Outfit">
    </div>
  </div>
  <ul class="js-looks-page">
    {% for product in collections.look1.products %}
      <li>
        <a href="{{ product.url }}" title="{{ product.title | escape }} &mdash; {{ product.description | strip_html }}">
          <img src="{{ product.images.first | product_img_url: 'large' }}" alt="{{ product.title | escape }}" />
        </a>
        <div>
          <div>
            <a href="{{ product.url }}">
              {{ product.title }}  
            </a>
            <span class="money serif">{{ product.price | money | remove: '.00' }}</span>
          </div>
          {% include'add-to-cart' %}
        </div>
      </li>
    {% endfor %}
  </ul>
