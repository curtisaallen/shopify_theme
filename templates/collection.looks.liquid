<section class="collection_looks">
  <div>
    <img class="hero" src="{{ 'look1.jpg' | asset_url }}" />
    <div class="description right">
      <h4 class="white">&ldquo;Stand out in a sea of gray and black.&rdquo;</h4>
      <p class="white">Riley blazer <span>$228</span>, Riley pencil skirt <span>$128</span></p>
      <input type="button" name="Buy Outfit" value="Buy This Outfit">
    </div>
  </div>
  <ul>
    {% for product in collections.look1.products %}
      <li>
        <a href="{{ product.url }}" title="{{ product.title | escape }} &mdash; {{ product.description | strip_html }}">
          <img src="{{ product.images.first | product_img_url: 'large' }}" alt="{{ product.title | escape }}" />
        </a>
        <div>
          <a href="{{ product.url }}">
            {{ product.title }}  
          </a>
          <span class="money">{{ product.price | money | remove: '.00' }}</span>
          {% include'add-to-cart' %}
        </div>
      </li>
    {% endfor %}
  </ul>
